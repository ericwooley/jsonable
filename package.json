{
  "name": "jsonable",
  "version": "0.0.3",
  "description": "Composable json via javascript",
  "main": "dist/index.js",
  "author": "",
  "bin": {
    "jsonjs": "./dist/bin.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com:ericwooley/jsonable.git"
  },
  "scripts": {
    "test": "rm -rf dist && standard src/ && npm run build && istanbul cover _mocha --recursive dist/tests",
    "build": "mkdir -p dist/tests/files && cp src/tests/files/*.json dist/tests/files && babel src --out-dir dist --source-maps && echo '\\n Finished transpile \\n'",
    "watch": "nodemon --watch src/ --exec \"npm run test\"",
    "prepublish": "npm run build"
  },
  "license": "ISC",
  "dependencies": {
    "mkdirp": "^0.5.1",
    "minimist": "^1.2.0"
  },
  "devDependencies": {
    "babel": "^6.1.18",
    "babel-cli": "^6.1.18",
    "babel-plugin-transform-es2015-modules-umd": "^6.1.18",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-stage-0": "^6.1.18",
    "chai": "^3.4.1",
    "istanbul": "^0.4.0",
    "mocha": "^2.3.3",
    "nodemon": "^1.8.1",
    "npm-watch": "0.0.1",
    "standard": "^5.3.1"
  }
}
